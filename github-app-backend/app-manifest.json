{
  "name": "Claude Todo Implementation Bot",
  "url": "https://github.com/Vbraniumlads/vibetorch",
  "hook_attributes": {
    "url": "https://vibetorch.xyz/webhook"
  },
  "redirect_url": "https://vibetorch.xyz/callback",
  "callback_urls": [
    "https://vibetorch.xyz/callback"
  ],
  "request_oauth_on_install": false,
  "setup_on_update": true,
  "description": "Automatically creates implementation repositories from todo items mentioning @claude",
  "public": true,
  "default_events": [
    "push",
    "issues",
    "issue_comment",
    "pull_request",
    "installation",
    "installation_repositories"
  ],
  "default_permissions": {
    "actions": "read",
    "administration": "read",
    "contents": "write",
    "issues": "write",
    "metadata": "read",
    "pull_requests": "write",
    "repository_projects": "write"
  }
}